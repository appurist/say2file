    },
    {
      "type": "node",
      "request": "launch",
      "name": "Eleven Small MP3",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}\\say2file.js",
      "args": ["-p", "11", "-t", "mp3", "Hello there, this is a test."]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Eleven Small WAV",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}\\say2file.js",
