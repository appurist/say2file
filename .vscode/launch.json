  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "IBM",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}\\say2file.js",
      "args": [ "-f", "sample.txt", "-s"]
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Eleven Small",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}\\say2file.js",
      "args": ["-p", "11", "Hello there, this is a test."]
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Eleven",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}\\say2file.js",
      // "args": ["-p", "11", "Hello there, this is a test."]
      "args": ["-p", "11", "-f", "sample.txt", "-s"]
    }
  ]
}
